<Window
    x:Class="RengaLookup.UI.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:RengaLookup.UI"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:viewModel="clr-namespace:RengaLookup.UI.ViewModel"
    Title="MainWindow"
    Width="800"
    Height="450"
    mc:Ignorable="d">
    <Window.DataContext>
        <viewModel:ViewModel />
    </Window.DataContext>

    <Grid>
        <TextBlock
            Width="200"
            Height="200"
            Text="{Binding SomeText}" />

    </Grid>
</Window>
